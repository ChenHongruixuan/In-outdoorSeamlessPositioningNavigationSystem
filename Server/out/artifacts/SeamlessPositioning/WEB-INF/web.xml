<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>server.IndoorMapHttpServlet</servlet-name>
        <servlet-class>server.IndoorMapHttpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>server.IndoorMapHttpServlet</servlet-name>
        <url-pattern>/servlet/IndoorMapHttpServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>server.LocateDataPickServlet</servlet-name>
        <servlet-class>server.LocateDataPickServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>server.LocateDataPickServlet</servlet-name>
        <url-pattern>/servlet/LocateDataPickServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>server.IndoorLocateHttpServlet</servlet-name>
        <servlet-class>server.IndoorLocateHttpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>server.IndoorLocateHttpServlet</servlet-name>
        <url-pattern>/servlet/IndoorLocateHttpServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>server.NavigationHttpServlet</servlet-name>
        <servlet-class>server.NavigationHttpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>server.NavigationHttpServlet</servlet-name>
        <url-pattern>/servlet/NavigationHttpServlet</url-pattern>
    </servlet-mapping>

</web-app>